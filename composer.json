{
    "name": "jmleroux/jmlshopping",
    "description": "My shopping list",
    "homepage": "https://github.com/jmleroux/jml-shopping",
    "require": {
        "php": ">=7.3",
        "ext-pdo": "*",
        "ext-json": "*",
        "defuse/php-encryption": "@stable",
        "doctrine/doctrine-bundle": "@stable",
        "symfony/dotenv": "^4.1",
        "symfony/flex": "^1.9",
        "symfony/monolog-bundle": "^3.3",
        "symfony/security-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/webpack-encore-bundle": "^1.2"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^5.0",
        "leanphp/phpspec-code-coverage": "@stable",
        "phpspec/phpspec": "@stable",
        "symfony/browser-kit": "^4.1",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "^4.1"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "Jmleroux\\JmlShopping\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmleroux\\JmlShopping\\Api\\Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
