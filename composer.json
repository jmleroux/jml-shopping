{
    "name": "jmleroux/jmlshopping",
    "description": "My shopping list",
    "homepage": "https://github.com/jmleroux/jml-shopping",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
        "doctrine/dbal": "@stable",
        "silex/silex": "~2.0",
        "defuse/php-encryption": "@stable",
        "zendframework/zend-hydrator": "@stable",
        "cilex/console-service-provider": "~2.0@dev",
        "monolog/monolog": "@stable"
    },
    "require-dev": {
        "phpspec/phpspec": "@stable",
        "henrikbjorn/phpspec-code-coverage": "@stable"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Jmleroux\\": "src/Jmleroux"
        }
    }
}
